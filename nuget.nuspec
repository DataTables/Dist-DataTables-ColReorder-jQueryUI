<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.5.4</version>
        <id>datatables.net-colreorder-jqui</id>
        <description>This is ColReorder for DataTables with styling for [jQueryUI](http://jqueryui.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-ColReorder-jQueryUI.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>ColReorder  Reorder  jQuery UI  jQueryUI  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-colreorder" version=">=1.5.4"/>
            <dependency id="datatables.net-jqui" version=">=1.10.25"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/colReorder.jqueryui.js" target="content\Scripts"/>
        <file src="js/colReorder.jqueryui.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/colReorder.jqueryui.min.js" target="content\Scripts"/>
        <file src="js/colReorder.jqueryui.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/colReorder.jqueryui.css" target="content\Content"/>
        <file src="css/colReorder.jqueryui.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/colReorder.jqueryui.min.css" target="content\Content"/>
        <file src="css/colReorder.jqueryui.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/colReorder.jQueryUI.d.ts" target="content\Scripts"/>
        <file src="types/colReorder.jQueryUI.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

